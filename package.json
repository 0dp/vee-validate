{
  "name": "vue-validation",
  "version": "1.0.0",
  "description": "Simple VueJS input validation plugin",
  "main": "dist/vue-validation.js",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --config webpack.docs.config.js",
    "dev": "webpack --progress --colors --watch",
    "test": "nyc ava --colors",
    "test:watch": "ava --watch",
    "lint": "eslint ./src ./test",
    "build": "webpack && cross-env NODE_ENV=production webpack -p",
    "docs": "webpack --config webpack.docs.config.js",
    "docs:production": "cross-env NODE_ENV=production webpack -p --config webpack.docs.config.js",
    "build:all": "npm run build && npm run docs"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "keywords": [
    "VueJS",
    "Vue",
    "validation",
    "validator",
    "inputs",
    "form"
  ],
  "author": "Abdelrahman Awad <logaretm1@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "animate.css": "^3.5.1",
    "ava": "^0.15.2",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.13.2",
    "collectionsjs": "^0.3.2",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb-base": "^5.0.0",
    "eslint-plugin-import": "^1.12.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "node-sass": "^3.8.0",
    "nyc": "^7.1.0",
    "prismjs": "^1.5.1",
    "purecss": "^0.6.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue": "^1.0.26",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^8.5.3",
    "vue-style-loader": "^1.0.0",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-merge": "^0.14.1"
  }
}
