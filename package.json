{
  "name": "vee-validate",
  "version": "2.0.0-beta.11",
  "description": "Simple Vue.js input validation plugin",
  "main": "dist/vee-validate.js",
  "homepage": "http://vee-validate.logaretm.com",
  "repository": "https://github.com/logaretm/vee-validate",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --config webpack.docs.config.js",
    "dev": "webpack --progress --colors --watch",
    "test": "nyc ava --colors",
    "test:watch": "ava --watch",
    "lint": "eslint ./src ./test",
    "build": "webpack && cross-env NODE_ENV=production webpack -p",
    "cover": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "keywords": [
    "VueJS",
    "Vue",
    "validation",
    "validator",
    "inputs",
    "form"
  ],
  "author": "Abdelrahman Awad <logaretm1@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.13.2",
    "cross-env": "^2.0.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb-base": "^5.0.0",
    "eslint-plugin-import": "^1.12.0",
    "moment": "^2.14.1",
    "nyc": "^7.1.0",
    "webpack": "^2.1.0-beta.22"
  }
}
